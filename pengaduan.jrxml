	<queryString language="SQL">
		<![CDATA[SELECT
     pengaduan.`id_pengaduan` AS pengaduan_id_pengaduan,
     pengaduan.`tgl_pengaduan` AS pengaduan_tgl_pengaduan,
     pengaduan.`nik` AS pengaduan_nik,
     pengaduan.`isi_laporan` AS pengaduan_isi_laporan,
     pengaduan.`status` AS pengaduan_status,
     tanggapan.`id_tanggapan` AS tanggapan_id_tanggapan,
     tanggapan.`id_pengaduan` AS tanggapan_id_pengaduan,
     tanggapan.`tgl_tanggapan` AS tanggapan_tgl_tanggapan,
     tanggapan.`tanggapan` AS tanggapan_tanggapan,
     tanggapan.`id_petugas` AS tanggapan_id_petugas,
     masyarakat.`nik` AS masyarakat_nik,
     masyarakat.`nama` AS masyarakat_nama,
     masyarakat.`username` AS masyarakat_username,
     masyarakat.`password` AS masyarakat_password,
     masyarakat.`telp` AS masyarakat_telp,
     masyarakat.`level` AS masyarakat_level
FROM
     `pengaduan` pengaduan INNER JOIN `tanggapan` tanggapan ON pengaduan.`id_pengaduan` = tanggapan.`id_pengaduan`
     INNER JOIN `masyarakat` masyarakat ON pengaduan.`nik` = masyarakat.`nik`]]>
	</queryString>
